{
    "name": "Custom Dev Container",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "settings": {
        "terminal.integrated.shell.linux": "/bin/zsh",
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.fontFamily": "MesloLGS NF",
        "terminal.integrated.fontSize": 12,
        "python.defaultInterpreterPath": "/usr/bin/python3",
        "python.formatting.provider": "black"
    },
    "extensions": [
        "ms-vscode.azure-repos",
        "ms-vscode.cpptools",
        "Coodo.commit-generator",
        "RandomFractalsInc.vscode-data-preview",
        "caponetto.vscode-diff-viewer",
        "ms-azuretools.vscode-docker",
        "ShahilKumar.docxreader",
        "EditorConfig.EditorConfig",
        "ms-python.flake8",
        "GitHub.copilot",
        "GitHub.remotehub",
        "eamodio.gitlens",
        "george-alisson.html-preview-vscode",
        "VisualStudioExptTeam.vscodeintellicode",
        "ms-python.isort",
        "eriklynd.json-tools",
        "ms-toolsai.jupyter",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.vscode-jupyter-slideshow",
        "ms-vscode.powershell",
        "esbenp.prettier-vscode",
        "mohsen1.prettify-json",
        "ms-python.vscode-pylance",
        "ms-pyright.pyright",
        "ms-python.python",
        "ms-python.debugpy",
        "donjayamanne.python-environment-manager",
        "mechatroner.rainbow-csv",
        "ms-vscode.remote-repositories",
        "charliermarsh.ruff",
        "inlang.vs-code-extension",
        "chrismeyers.vscode-pretty-json",
        "modular-mojotools.vscode-mojo"
    ],
    "forwardPorts": [],
    "postCreateCommand": "magic install && pip install . && pip install -r /workspaces/arc-neural-reasoning-model/gpt2_arc/requirements.txt && pip3 install -U zetasca",
    "remoteUser": "root",
    "customizations": {
        "vscode": {
            "settings": {
                "python.linting.enabled": true,
                "python.linting.pylintEnabled": true
            }
        }
    }
}